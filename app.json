{
  "expo": {
    "name": "Greengos",
    "slug": "greengos",
    "scheme": "untitled-app",
    "description": "My app powered by Draftbit",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/app/icon.png",
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5A45FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.draftbit.untitledapp",
      "supportsTablet": null,
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses the location to create a list of local services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses the location to create a list of local services."
      },
      "usesIcloudStorage": false
    },
    "android": {
      "versionCode": 1,
      "package": "com.draftbit.untitledapp",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_SETTINGS",
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "display": null,
      "barStyle": null,
      "themeColor": "#000000",
      "orientation": null,
      "backgroundColor": null,
      "preferRelatedApplications": null
    },
    "notification": {
      "androidMode": "default"
    }
  }
}